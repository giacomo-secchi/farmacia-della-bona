{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"gradients": [
				{
					"slug": "gradient-1",
					"gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--primary-light-100) 100%)",
					"name": "Vertical soft beige to white"
				},
				{
					"slug": "gradient-2",
					"gradient": "linear-gradient(to bottom, var(--wp--preset--color--base) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Vertical soft beige to white"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#555555",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#348d70",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#fbfefd",
					"name": "Primary Light (100)",
					"slug": "primary-light-100"
				},
				{
					"color": "#daf1e9",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#f5f5f5",
					"name": "Neutral",
					"slug": "neutral"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Helvetica Neue\", Helvetica, Arial, Inter, sans-serif",
					"name": "Helvetica Neue",
					"slug": "helvetica-neue"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/inter/inter-200-normal.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter/inter-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/inter/inter-600-normal.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/inter/inter-200-italic.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/inter/inter-400-italic.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/inter/inter-600-italic.woff2"
							]
						}
					],
					"fontFamily": "Inter, sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast) !important"
							}
						}
					}
				}
			}
		},
		"elements": {
			"link": {
				"css": "a, a:not(.wp-element-button) {text-decoration: none;}"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--helvetica-neue)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "300",
			"lineHeight": "var(--wp--custom--line-height--large)"
		}
	}
}